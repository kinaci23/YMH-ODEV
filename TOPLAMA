package toplama.proje7;
// Tanımlanan iki sayının toplamını veren java kodu
public class Proje7 {
  public static void main(String[] args) {
 
        int sayi1 = 10;
        int sayi2 = 20;
 
        System.out.println("Sayılar : " + sayi1 + " " + sayi2);
        int toplam = sayi1 + sayi2;
 
        System.out.println("Toplam: " + toplam);
  
    }
}
